<?xml version="1.0" encoding="UTF-8"?>
<xp:view
	xmlns:xp="http://www.ibm.com/xsp/core"
	xmlns:xe="http://www.ibm.com/xsp/coreex"
	xmlns:xc="http://www.ibm.com/xsp/custom">
	<xp:div
		id="divTaskWrapper">
		<xe:switchFacet
			id="switchTask"
			selectedFacet="#{App.taskCon.facet}">
			<xp:this.facets>
				<xc:cc2TaskView
					xp:key="view"></xc:cc2TaskView>
				<xc:cc2TaskForm
					xp:key="form"></xc:cc2TaskForm>
			</xp:this.facets>
		</xe:switchFacet>
	</xp:div>
</xp:view>